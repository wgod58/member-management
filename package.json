{
  "name": "member-management",
  "version": "0.1.3",
  "private": true,
  "scripts": {
    "postinstall": "husky install",
    "prebuild": "rimraf dist",
    "build": "babel src --out-dir dist --source-maps --copy-files",
    "start": " babel-node ./src/index.js",
    "start:docker": "node ./index.js",
    "run:watch": "nodemon --exec yarn start",
    "lint": "esw --ignore-path .gitignore .",
    "lint:fix": "eslint --fix \"./src/**/*.js\" ",
    "test": "NODE_ENV=testing jest src --setup-files dotenv/config --detectOpenHandles",
    "jest": "jest",
    "coverage": "NODE_ENV=testing jest src --coverage --setup-files dotenv/config --detectOpenHandles",
    "prettier": "prettier --write \"src/**/*.js\""
  },
  "jest": {
    "clearMocks": true,
    "verbose": true,
    "coveragePathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/dist/"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ],
    "setupFilesAfterEnv": [
      "./jest.setup.afterEnv.js"
    ],
    "testTimeout": 30000
  },
  "dependencies": {
    "bunyan": "^1.8.14",
    "compression": "^1.7.0",
    "cors": "^2.8.4",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-healthcheck": "^0.1.0",
    "helmet": "^4.4.1",
    "morgan": "^1.8.2",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.4.0",
    "swagger-ui-express": "^4.1.2",
    "version-healthcheck": "^0.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/runtime": "^7.9.2",
    "@types/jest": "^26.0.20",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-watch": "^7.0.0",
    "husky": "^5.1.3",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.1",
    "prettier": "^2.2.1"
  }
}
